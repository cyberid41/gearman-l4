{
    "name": "photomania/gearman-l4",
    "description": "a GearmanClient wrapper for Laravel 4",
    "license": "MIT",
    "authors": [
        {
            "name": "Bar Ziony",
            "email": "bar@photomania.net"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.0.x"
    },
    "suggest": {
        "ext-gearman": "The Gearman PECL extension is needed for this package to work, but is listed as suggested to allow `composer install` on development machines."
    },
    "autoload": {
        "psr-0": {
            "Photomania\\GearmanL4": "src/"
        }
    },
    "minimum-stability": "dev"
}
